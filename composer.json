{
    "name": "szemul/templating",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0|^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "phpstan/phpstan": "^1.1.0",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\Templating\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\Templating\\Test\\": "test"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": false
    }
}
